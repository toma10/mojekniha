{
    "name": "toma10/mojekniha",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "require": {
        "php": "^7.2",
        "ebess/advanced-nova-media-library": "^2.9",
        "fideloper/proxy": "^4.0",
        "inertiajs/inertia-laravel": "^0.2.5",
        "laravel/framework": "^6.2",
        "laravel/nova": "*",
        "laravel/tinker": "^1.0",
        "spatie/data-transfer-object": "^1.10",
        "spatie/laravel-medialibrary": "^7.0.0",
        "tymon/jwt-auth": "1.0.0-rc.5"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.5.2",
        "nunomaduro/phpinsights": "^1.11",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "php-cs-fixer fix -v",
        "test:lint": "php-cs-fixer fix -v --dry-run",
        "test:types": "phpstan analyse",
        "test:insights": "php artisan insights",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:insights"
        ]
    }
}
